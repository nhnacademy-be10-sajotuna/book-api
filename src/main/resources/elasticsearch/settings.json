{
  "analysis": {
    "analyzer": {
      "korean_icu_analyzer": {
        "type": "custom",
        "tokenizer": "nori_tokenizer",
        "filter": [
          "lowercase",
          "icu_folding"
        ]
      },
      "jaso_analyzer": {
        "type": "custom",
        "tokenizer": "jaso_tokenizer"
      },
      "synonym_analyzer": {
        "type": "custom",
        "tokenizer": "nori_tokenizer",
        "filter": [
          "lowercase",
          "synonym_filter"
        ]
      }
    },
    "filter": {
      "synonym_filter": {
        "type": "synonym",
        "synonyms": [
          "히히, 하하",
          "아기, 유아",
          "학생, 제자",
          "구입, 구매",
          "예쁜, 아름다운",
          "슬픈, 우울한",
          "기질, 특성",
          "LA, 로스엔젤레스"
        ]
      }
    }
  }
}
